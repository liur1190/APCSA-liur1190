import java.lang.System;
import java.lang.Math;

public class labassessment1
{
	public static void main( String args[] )
	{
		//add test cases
		Numbers.isGoofy(12);
		Numbers.isGoofy(15);
		Numbers.isGoofy(26);
		Numbers.isGoofy(8);
		Numbers.isGoofy(1234);
		
		Numbers.getSomeGoofyNumbers(3);
		Numbers.getSomeGoofyNumbers(15);
		
	}

}

public class Numbers
{
	
	
	public Numbers()
	{
		
	}
	
	
	public static boolean isGoofy(int num)
	{
	
		
		for (int i=0; i < num.length(); i++)
			if (num / num.substring(0,i))
				sum += num(i);
		
		
			
		
		if (sum % 2 == 0)
			return false;
		if (sum % 2 == 1)
			return true;
				
	}
	
	public static int[] getSomeGoofyNumbers(int count)
	{
		int[] goofies = new int[count];
		int stop = count;
		while (stop > 0)
			for (int i = 0; i < 100; i++)
				if (isGoofy(i) == false)
					goofies[i] = i;
					stop--;
		
		return goofies;
			
	}
}
