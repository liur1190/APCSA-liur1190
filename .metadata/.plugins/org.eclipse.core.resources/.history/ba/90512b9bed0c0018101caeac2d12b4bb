import static java.lang.System.*;

public class Name
{
	private String name;

	public Name()
	{
		name = "";

	}

	public Name(String s)
	{
		setName("");

	}

   public void setName(String s)
   {
	   name = s;

   }

	public String getFirst()
	{
		return name.substring(0, name.lastIndexOf("  "));
	}

	public String getLast()
	{
		return "";
	}

 	public String toString()
 	{
 		return "";
	}
}